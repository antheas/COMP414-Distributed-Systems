{
  "name": "bored_games_game_server",
  "version": "1.0.0",
  "description": "A distributed board gaming service.",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "start": "npm run build && nodemon dist/index.js",
    "build": "babel src -d dist --extensions \".ts,.tsx,.js\"",
    "serve": "node dist/index.js",
    "restart": "npm run build && npm run serve",
    "dev": "nodemon --ignore './dist/' --ext ts,js --verbose --exec \"npm run restart\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/chess.js": "^0.10.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/ip": "^1.1.0",
    "@types/socket.io": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "axios": "^0.19.2",
    "chess.js": "^0.10.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "node-zookeeper-client": "^1.1.0",
    "socket.io": "^2.3.0"
  }
}
